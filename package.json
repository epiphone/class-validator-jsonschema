{
  "name": "class-validator-jsonschema",
  "version": "3.1.1",
  "description": "Convert class-validator-decorated classes into JSON schema",
  "keywords": [
    "class-validator",
    "jsonschema",
    "openapi",
    "swagger"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "git@github.com:epiphone/class-validator-jsonschema.git",
  "author": "Aleksi Pekkala <aleksipekkala@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf coverage build",
    "build": "npm run clean && tsc -p tsconfig.release.json",
    "format": "prettier --write {src,__tests__}/**/*.ts",
    "test:format": "prettier --check {src,__tests__}/**/*.ts",
    "test:lint": "tslint --project . src/**/*.ts",
    "prepare": "npm run build",
    "send-coverage": "codecov -f coverage/*.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "openapi3-ts": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^14.14.7",
    "@types/prettier": "^2.7.2",
    "@types/reflect-metadata": "^0.1.0",
    "@types/validator": "^13.7.2",
    "codecov": "^3.8.3",
    "jest": "^29.3.1",
    "lodash.get": "^4.4.2",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "class-transformer": "^0.4.0 || ^0.5.0",
    "class-validator": "^0.14.0"
  }
}
