{
  "name": "class-validator-jsonschema",
  "version": "1.2.1",
  "description": "Convert class-validator-decorated classes into JSON schema",
  "keywords": [
    "class-validator",
    "jsonschema",
    "openapi",
    "swagger"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "git@github.com:epiphone/class-validator-jsonschema.git",
  "author": "Aleksi Pekkala <aleksipekkala@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf coverage build",
    "build": "yarn clean && tsc -p tsconfig.release.json",
    "format": "prettier --write {src,test}/**/*.ts",
    "lint": "tslint --project . src/**/*.ts",
    "prepare": "install-self-peers -- --ignore-scripts && yarn build",
    "send-coverage": "codecov -f coverage/*.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "debug": "^4.1.1",
    "lodash": "^4.17.11",
    "openapi3-ts": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@team-griffin/install-self-peers": "^1.1.1",
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.134",
    "@types/prettier": "^1.16.4",
    "@types/reflect-metadata": "^0.1.0",
    "@types/rimraf": "^2.0.2",
    "codecov": "^3.5.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tsutils": "^3.14.0",
    "typescript": "^3.5.2"
  },
  "peerDependencies": {
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1"
  }
}
